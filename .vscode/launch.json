  "version": "0.2.0",
  "configurations": [
    {
      "name": "ng serve",
      "type": "chrome",
      "request": "launch",
      "preLaunchTask": "npm: start:connected",
      "url": "localhost:3000",
      "port": 3000,
      "webRoot": "${workspaceFolder}/src/easybank",
      "sourceMapPathOverrides": {
        "webpack:///./src/*": "${webRoot}/*"
      },
      "sourceMaps": true,
    },
  ]
}
  
  